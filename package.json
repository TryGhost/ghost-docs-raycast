{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ghost-docs",
  "title": "ghost-docs",
  "description": "Search Ghost help, resources, tutorials, and developer docs",
  "icon": "command-icon.png",
  "author": "ryan_feigenbaum",
  "owner": "tryghost",
  "categories": [
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "ghost",
      "subtitle": "Search ghost docs",
      "description": "Search Ghost help, resources, tutorials, and developer docs",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.62.1",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}